    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Data properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.semanticweb.org/oriol.ruiz/ontologies/2025/6/basic_ontology#hasGenericDataProp -->

    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/oriol.ruiz/ontologies/2025/6/basic_ontology#hasGenericDataProp"/>
    


    <!-- http://www.semanticweb.org/oriol.ruiz/ontologies/2025/6/basic_ontology#special_object -->

    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/oriol.ruiz/ontologies/2025/6/basic_ontology#special_object">
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#boolean"/>
        <rdfs:comment>Boolean flag to identify special objects for testing - set to true for can2, false for can1</rdfs:comment>
    </owl:DatatypeProperty>
    


    <!-- http://www.semanticweb.org/oriol.ruiz/ontologies/2025/6/basic_ontology#test_mode -->

    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/oriol.ruiz/ontologies/2025/6/basic_ontology#test_mode">
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#boolean"/>
        <rdfs:comment>Boolean flag to enable flavor testing - set to true for robot2, false for robot1</rdfs:comment>
    </owl:DatatypeProperty>
    


    <!-- http://www.semanticweb.org/oriol.ruiz/ontologies/2025/6/basic_ontology#test_mode_2 -->

    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/oriol.ruiz/ontologies/2025/6/basic_ontology#test_mode_2">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topDataProperty"/>
    </owl:DatatypeProperty>
    


    <!-- http://www.semanticweb.org/oriol.ruiz/ontologies/2025/6/basic_ontology#testing_mode_2 -->

    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/oriol.ruiz/ontologies/2025/6/basic_ontology#testing_mode_2">
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#boolean"/>
        <rdfs:comment>Boolean flag for secondary testing mode - set to true for can3</rdfs:comment>
    </owl:DatatypeProperty>
    


