    <!-- http://www.semanticweb.org/oriol.ruiz/ontologies/2025/6/basic_ontology#can1 -->

    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/oriol.ruiz/ontologies/2025/6/basic_ontology#can1">
        <rdf:type rdf:resource="http://www.semanticweb.org/oriol.ruiz/ontologies/2025/6/basic_ontology#ManipulationObject"/>
        <hasGenericObjProp rdf:resource="http://www.semanticweb.org/oriol.ruiz/ontologies/2025/6/basic_ontology#robot1"/>
        <hasGenericDataProp>VALUE</hasGenericDataProp>
        <special_object rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean">false</special_object>
        <rdfs:comment>Standard object - special_object=false, flavors will NOT trigger in EC1</rdfs:comment>
    </owl:NamedIndividual>
    


    <!-- http://www.semanticweb.org/oriol.ruiz/ontologies/2025/6/basic_ontology#can2 -->

    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/oriol.ruiz/ontologies/2025/6/basic_ontology#can2">
        <rdf:type rdf:resource="http://www.semanticweb.org/oriol.ruiz/ontologies/2025/6/basic_ontology#ManipulationObject"/>
        <special_object rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean">true</special_object>
        <rdfs:comment>Special test object - special_object=true, flavors WILL trigger in EC2</rdfs:comment>
    </owl:NamedIndividual>
    


    <!-- http://www.semanticweb.org/oriol.ruiz/ontologies/2025/6/basic_ontology#can3 -->

    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/oriol.ruiz/ontologies/2025/6/basic_ontology#can3">
