        <rdf:type rdf:resource="http://www.semanticweb.org/oriol.ruiz/ontologies/2023/8/beaware_ontology#Ignore"/>
        <beaw:hasTargetBehavior rdf:resource="http://www.semanticweb.org/oriol.ruiz/ontologies/2025/6/basic_ontology#p_primitive_l"/>
        <beaw:isFlavorInMode rdf:resource="http://www.semanticweb.org/oriol.ruiz/ontologies/2025/6/basic_ontology#cm_context_mode_gamma"/>
        <beaw:isFlavorOf rdf:resource="http://www.semanticweb.org/oriol.ruiz/ontologies/2025/6/basic_ontology#b_high_behavior_y"/>
        <beaw:hasTrigger>FORCETRUE</beaw:hasTrigger>
    </owl:NamedIndividual>
    


    <!-- http://www.semanticweb.org/oriol.ruiz/ontologies/2025/6/basic_ontology#mrs_flavor_10_actionrecovery_m_i_terminal -->

