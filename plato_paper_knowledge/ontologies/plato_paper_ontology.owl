        <beaw:hasContextTrigger rdf:datatype="http://www.w3.org/2001/XMLSchema#string">FORCETRUE</beaw:hasContextTrigger>
    </owl:NamedIndividual>
    


    <!-- http://www.semanticweb.org/oriol.ruiz/ontologies/2025/6/basic_ontology#flavor_1_delete_m -->

    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/oriol.ruiz/ontologies/2025/6/basic_ontology#flavor_1_delete_m">
        <rdf:type rdf:resource="http://www.semanticweb.org/oriol.ruiz/ontologies/2023/8/beaware_ontology#Flavor"/>
        <rdf:type rdf:resource="http://www.semanticweb.org/oriol.ruiz/ontologies/2023/8/beaware_ontology#Ignore"/>
        <beaw:hasTargetBehavior rdf:resource="http://www.semanticweb.org/oriol.ruiz/ontologies/2025/6/basic_ontology#p_primitive_m"/>
        <beaw:isFlavorOf rdf:resource="http://www.semanticweb.org/oriol.ruiz/ontologies/2025/6/basic_ontology#b_high_behavior_x"/>
        <beaw:hasTrigger rdf:datatype="http://www.w3.org/2001/XMLSchema#string">ASK WHERE { &lt;manipulationobject&gt; rdf:type onto:ManipulationObject . &lt;manipulationobject&gt; onto:special_object true }</beaw:hasTrigger>
        <rdfs:comment>Deletes/ignores Primitive M when object has special_object=true (can2 only, not can1)</rdfs:comment>
    </owl:NamedIndividual>
    


    <!-- http://www.semanticweb.org/oriol.ruiz/ontologies/2025/6/basic_ontology#flavor_2_switch_b_c -->

    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/oriol.ruiz/ontologies/2025/6/basic_ontology#flavor_2_switch_b_c">
        <rdf:type rdf:resource="http://www.semanticweb.org/oriol.ruiz/ontologies/2023/8/beaware_ontology#Flavor"/>
        <rdf:type rdf:resource="http://www.semanticweb.org/oriol.ruiz/ontologies/2023/8/beaware_ontology#SwitchAandB"/>
        <beaw:hasTargetBehaviorA rdf:resource="http://www.semanticweb.org/oriol.ruiz/ontologies/2025/6/basic_ontology#b_behavior_b"/>
        <beaw:hasTargetBehaviorB rdf:resource="http://www.semanticweb.org/oriol.ruiz/ontologies/2025/6/basic_ontology#b_behavior_c"/>
        <beaw:isFlavorOf rdf:resource="http://www.semanticweb.org/oriol.ruiz/ontologies/2025/6/basic_ontology#b_high_behavior_x"/>
        <beaw:hasTrigger rdf:datatype="http://www.w3.org/2001/XMLSchema#string">ASK WHERE { &lt;robot&gt; rdf:type onto:Robot . &lt;robot&gt; onto:test_mode true }</beaw:hasTrigger>
        <rdfs:comment>Replaces Behavior B with Behavior C when robot has test_mode=true (robot2 only, not robot1)</rdfs:comment>
    </owl:NamedIndividual>
    


    <!-- http://www.semanticweb.org/oriol.ruiz/ontologies/2025/6/basic_ontology#flavor_3_delete_a_in_cm_alpha -->

    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/oriol.ruiz/ontologies/2025/6/basic_ontology#flavor_3_delete_a_in_cm_alpha">
        <rdf:type rdf:resource="http://www.semanticweb.org/oriol.ruiz/ontologies/2023/8/beaware_ontology#Ignore"/>
        <beaw:hasTargetBehavior rdf:resource="http://www.semanticweb.org/oriol.ruiz/ontologies/2025/6/basic_ontology#b_behavior_a"/>
        <beaw:isFlavorInMode rdf:resource="http://www.semanticweb.org/oriol.ruiz/ontologies/2025/6/basic_ontology#cm_context_mode_alpha"/>
        <beaw:isFlavorOf rdf:resource="http://www.semanticweb.org/oriol.ruiz/ontologies/2025/6/basic_ontology#b_high_behavior_y"/>
        <beaw:hasTrigger>EXPANSION</beaw:hasTrigger>
    </owl:NamedIndividual>
    


    <!-- http://www.semanticweb.org/oriol.ruiz/ontologies/2025/6/basic_ontology#flavor_4_replace_m_by_parameter_when_can1 -->

    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/oriol.ruiz/ontologies/2025/6/basic_ontology#flavor_4_replace_m_by_parameter_when_can1">
        <rdf:type rdf:resource="http://www.semanticweb.org/oriol.ruiz/ontologies/2023/8/beaware_ontology#ReplaceByParam"/>
        <beaw:hasTargetBehavior rdf:resource="http://www.semanticweb.org/oriol.ruiz/ontologies/2025/6/basic_ontology#p_primitive_m"/>
        <beaw:isFlavorInMode rdf:resource="http://www.semanticweb.org/oriol.ruiz/ontologies/2025/6/basic_ontology#cm_context_mode_alpha"/>
        <beaw:isFlavorOf rdf:resource="http://www.semanticweb.org/oriol.ruiz/ontologies/2025/6/basic_ontology#b_high_behavior_y"/>
        <beaw:hasTrigger>ASK WHERE { &lt;manipulationobject&gt; rdf:type onto:ManipulationObject . &lt;manipulationobject&gt; onto:special_object true }</beaw:hasTrigger>
        <beaw:obtainsParamValuewithQuery>SELECT#s(&quot;hello&quot;#sAS#s?greeting)#sWHERE#s{}</beaw:obtainsParamValuewithQuery>
        <beaw:replacedByParam>replacement_parameter</beaw:replacedByParam>
    </owl:NamedIndividual>
    


