        <testing_mode_2 rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean">true</testing_mode_2>
        <rdfs:comment>Special test object - testing_mode_2=true, mrs_flavor_11 WILL trigger</rdfs:comment>
    </owl:NamedIndividual>
    


    <!-- http://www.semanticweb.org/oriol.ruiz/ontologies/2025/6/basic_ontology#cm_context_mode_alpha -->

    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/oriol.ruiz/ontologies/2025/6/basic_ontology#cm_context_mode_alpha">
        <rdf:type rdf:resource="http://www.semanticweb.org/oriol.ruiz/ontologies/2023/8/beaware_ontology#ContextMode"/>
        <beaw:incompatibleWith rdf:resource="http://www.semanticweb.org/oriol.ruiz/ontologies/2025/6/basic_ontology#cm_context_mode_beta"/>
        <beaw:isContextModeOf rdf:resource="http://www.semanticweb.org/oriol.ruiz/ontologies/2025/6/basic_ontology#b_high_behavior_y"/>
    </owl:NamedIndividual>
    


    <!-- http://www.semanticweb.org/oriol.ruiz/ontologies/2025/6/basic_ontology#cm_context_mode_beta -->

