    </owl:Ontology>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Annotation properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.semanticweb.org/oriol.ruiz/ontologies/2023/8/beaware_ontology#hasReplacementBehavior -->

    <owl:AnnotationProperty rdf:about="http://www.semanticweb.org/oriol.ruiz/ontologies/2023/8/beaware_ontology#hasReplacementBehavior"/>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.semanticweb.org/oriol.ruiz/ontologies/2025/6/basic_ontology#PDDLProperties -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/oriol.ruiz/ontologies/2025/6/basic_ontology#PDDLProperties">
        <rdfs:comment>Parent property for all PDDL predicates</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/oriol.ruiz/ontologies/2025/6/basic_ontology#at_location -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/oriol.ruiz/ontologies/2025/6/basic_ontology#at_location">
        <rdfs:subPropertyOf rdf:resource="http://www.semanticweb.org/oriol.ruiz/ontologies/2025/6/basic_ontology#PDDLProperties"/>
        <rdfs:comment>PDDL predicate: robot is at a location</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/oriol.ruiz/ontologies/2025/6/basic_ontology#hasGenericObjProp -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/oriol.ruiz/ontologies/2025/6/basic_ontology#hasGenericObjProp"/>
    


    <!-- http://www.semanticweb.org/oriol.ruiz/ontologies/2025/6/basic_ontology#holding -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/oriol.ruiz/ontologies/2025/6/basic_ontology#holding">
        <rdfs:subPropertyOf rdf:resource="http://www.semanticweb.org/oriol.ruiz/ontologies/2025/6/basic_ontology#PDDLProperties"/>
        <rdfs:comment>PDDL predicate: robot is holding an object</rdfs:comment>
    </owl:ObjectProperty>
    


