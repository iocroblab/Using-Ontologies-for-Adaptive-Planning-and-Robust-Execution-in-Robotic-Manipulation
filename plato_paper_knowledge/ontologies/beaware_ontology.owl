    <!-- http://www.semanticweb.org/oriol.ruiz/ontologies/2023/8/beaware_ontology#Abstract -->

    <owl:Class rdf:about="http://www.semanticweb.org/oriol.ruiz/ontologies/2023/8/beaware_ontology#Abstract">
        <owl:disjointWith rdf:resource="http://www.semanticweb.org/oriol.ruiz/ontologies/2023/8/beaware_ontology#Physical"/>
        <rdfs:comment>Top-level class for abstract/conceptual entities (Behaviors, Primitives, Flavors, ContextModes, Parameters). Disjoint from Physical entities.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/oriol.ruiz/ontologies/2023/8/beaware_ontology#ActionMode -->

    <owl:Class rdf:about="http://www.semanticweb.org/oriol.ruiz/ontologies/2023/8/beaware_ontology#ActionMode">
        <owl:equivalentClass rdf:resource="http://www.semanticweb.org/oriol.ruiz/ontologies/2023/8/beaware_ontology#ContextMode"/>
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/oriol.ruiz/ontologies/2023/8/beaware_ontology#Abstract"/>
        <rdfs:comment>Synonym for ContextMode. Defines execution contexts for behaviors. ActionModes can be marked as incompatible (e.g., "slow" and "fast" cannot be active simultaneously).</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/oriol.ruiz/ontologies/2023/8/beaware_ontology#AddBtoA -->

    <owl:Class rdf:about="http://www.semanticweb.org/oriol.ruiz/ontologies/2023/8/beaware_ontology#AddBtoA">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/oriol.ruiz/ontologies/2023/8/beaware_ontology#SubbehaviorModifier"/>
        <rdfs:subClassOf>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.semanticweb.org/oriol.ruiz/ontologies/2023/8/beaware_ontology#hasDecorator"/>
                        <owl:hasValue>Fallback</owl:hasValue>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.semanticweb.org/oriol.ruiz/ontologies/2023/8/beaware_ontology#hasDecorator"/>
                        <owl:hasValue>Sequence</owl:hasValue>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.semanticweb.org/oriol.ruiz/ontologies/2023/8/beaware_ontology#hasDecorator"/>
                        <owl:hasValue>others?</owl:hasValue>
                    </owl:Restriction>
                </owl:unionOf>
            </owl:Class>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/oriol.ruiz/ontologies/2023/8/beaware_ontology#hasTargetBehaviorA"/>
                <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
                <owl:onClass rdf:resource="http://www.semanticweb.org/oriol.ruiz/ontologies/2023/8/beaware_ontology#Behavior"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/oriol.ruiz/ontologies/2023/8/beaware_ontology#hasTargetBehaviorB"/>
                <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
                <owl:onClass rdf:resource="http://www.semanticweb.org/oriol.ruiz/ontologies/2023/8/beaware_ontology#Behavior"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment>Loads the template for B and adds it TO THE RIGHT of A, with a decorator that has been selected with hasDecorator</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/oriol.ruiz/ontologies/2023/8/beaware_ontology#AddKinematicRestriction -->

    <owl:Class rdf:about="http://www.semanticweb.org/oriol.ruiz/ontologies/2023/8/beaware_ontology#AddKinematicRestriction">
